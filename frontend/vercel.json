{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ],
    "env": {
      "VITE_SOCKET_API": "https://your-backend-socket-server.com"
    }
  }
  